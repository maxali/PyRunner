{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerAppName": {
      "value": "pyrunner-prod"
    },
    "containerAppEnvironmentName": {
      "value": "pyrunner-env-prod"
    },
    "containerImage": {
      "value": "ghcr.io/gbmoalab/pyrunner:latest"
    },
    "minReplicas": {
      "value": 2
    },
    "maxReplicas": {
      "value": 20
    },
    "targetPort": {
      "value": 8000
    },
    "cpu": {
      "value": "2.0"
    },
    "memory": {
      "value": "4Gi"
    },
    "environment": {
      "value": "prod"
    }
  }
}