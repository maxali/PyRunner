{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerAppName": {
      "value": "pyrunner"
    },
    "containerAppEnvironmentName": {
      "value": "pyrunner-env"
    },
    "containerImage": {
      "value": "ghcr.io/maxali/pyrunner:latest"
    },
    "minReplicas": {
      "value": 1
    },
    "maxReplicas": {
      "value": 10
    },
    "targetPort": {
      "value": 8000
    },
    "cpu": {
      "value": "1.0"
    },
    "memory": {
      "value": "2Gi"
    },
    "environment": {
      "value": "dev"
    }
  }
}